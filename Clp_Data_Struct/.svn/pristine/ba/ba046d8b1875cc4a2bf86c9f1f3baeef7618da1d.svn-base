package com.bestinfo.bean.game;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 游戏-风险控制规则表(T_game_risk_rule)
 *
 * @author hhhh
 */
public class GameRiskRule implements Serializable {

    private static final long serialVersionUID = -920568115094412174L;
    private Integer game_id;//游戏编号
    private Integer group_id;//控制分组
    private String group_name;//分组名称
    private Integer play_num;//支持玩法数
    private Integer control_type;//控制方式
    private Integer ration_stake;//初始注数
    private BigDecimal ration_money;//初始投注金额
    private BigDecimal ration_prize;//初始风险奖金
    private BigDecimal increment_money;//每增加金额
    private BigDecimal increment_prize;//每增加奖金
    private Integer work_status;//工作状态

    public Integer getGame_id() {
        return game_id;
    }

    public void setGame_id(Integer game_id) {
        this.game_id = game_id;
    }

    public Integer getGroup_id() {
        return group_id;
    }

    public void setGroup_id(Integer group_id) {
        this.group_id = group_id;
    }

    public String getGroup_name() {
        return group_name;
    }

    public void setGroup_name(String group_name) {
        this.group_name = group_name;
    }

    public Integer getPlay_num() {
        return play_num;
    }

    public void setPlay_num(Integer play_num) {
        this.play_num = play_num;
    }

    public Integer getControl_type() {
        return control_type;
    }

    public void setControl_type(Integer control_type) {
        this.control_type = control_type;
    }

    public Integer getRation_stake() {
        return ration_stake;
    }

    public void setRation_stake(Integer ration_stake) {
        this.ration_stake = ration_stake;
    }

    public BigDecimal getRation_money() {
        return ration_money;
    }

    public void setRation_money(BigDecimal ration_money) {
        this.ration_money = ration_money;
    }

    public BigDecimal getRation_prize() {
        return ration_prize;
    }

    public void setRation_prize(BigDecimal ration_prize) {
        this.ration_prize = ration_prize;
    }

    public BigDecimal getIncrement_money() {
        return increment_money;
    }

    public void setIncrement_money(BigDecimal increment_money) {
        this.increment_money = increment_money;
    }

    public BigDecimal getIncrement_prize() {
        return increment_prize;
    }

    public void setIncrement_prize(BigDecimal increment_prize) {
        this.increment_prize = increment_prize;
    }

    public Integer getWork_status() {
        return work_status;
    }

    public void setWork_status(Integer work_status) {
        this.work_status = work_status;
    }

}
