package com.bestinfo.bean.game;

import java.io.Serializable;

/**
 * 游戏开奖次数
 *
 * @author chenliping
 */
public class PlayBetMode implements Serializable {

    /**
     *
     */
    private static final long serialVersionUID = -2584094131018016931L;
    private Integer game_id;//游戏编号
    private Integer play_id;//玩法编号
    private Integer bet_mode;//投注方式
    private Integer work_status;//工作状态

    private String bet_mode_name;//投注方式名称

    /**
     * @return the game_id
     */
    public Integer getGame_id() {
        return game_id;
    }

    /**
     * @param game_id the game_id to set
     */
    public void setGame_id(Integer game_id) {
        this.game_id = game_id;
    }

    /**
     * @return the play_id
     */
    public Integer getPlay_id() {
        return play_id;
    }

    /**
     * @param play_id the play_id to set
     */
    public void setPlay_id(Integer play_id) {
        this.play_id = play_id;
    }

    /**
     * @return the bet_mode
     */
    public Integer getBet_mode() {
        return bet_mode;
    }

    /**
     * @param bet_mode the bet_mode to set
     */
    public void setBet_mode(Integer bet_mode) {
        this.bet_mode = bet_mode;
    }

    /**
     * @return the work_status
     */
    public Integer getWork_status() {
        return work_status;
    }

    /**
     * @param work_status the work_status to set
     */
    public void setWork_status(Integer work_status) {
        this.work_status = work_status;
    }

    public String getBet_mode_name() {
        return bet_mode_name;
    }

    public void setBet_mode_name(String bet_mode_name) {
        this.bet_mode_name = bet_mode_name;
    }

}
