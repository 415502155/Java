/*a标签链接*/
a {
	color: #337ab7;
	background: 0 0;
	text-decoration: none;
	outline: 0;
	cursor: pointer;
	transition: color .2s ease
}

a:hover {
	color: #009ad9
}
a:active {
	color: #3091f2
}
a:active,a:hover {
	outline: 0;
	text-decoration: none
}
a[disabled] {
	color: #ccc;
	cursor: not-allowed;
	pointer-events: none
}



/*代码*/
code,kbd,pre,samp {
	font-family: Consolas,Menlo,Courier,monospace
}

/*文本对齐*/
.text-left { text-align: left;}
.text-right { text-align: right;}
.text-center { text-align: center;}


/* 说明文档 左侧菜单 */
.header {
	height: 65px;
	background-color: #337ab7;
	color: #fff;
	line-height: 65px;
	padding: 0 30px;
	box-shadow: -1px -2px 3px 5px rgba(0, 0, 0, 0.25);
}
.bs-docs-header {
	position: relative;
	padding: 30px 15px;
	color: #909090;
	text-align: center;
	text-shadow: 0 1px 0 rgba(0,0,0,.1);
	/* background-color: #6f5499; */
	background:#f2f2f2 url(../img/bannerbg.png);
	z-index: -1;
}
.bs-docs-header h1 {
	margin-top: 0;
	color: #000;
}
.bs-docs-header p {
	font-size: 18px;
}
.container {
	padding-right: 15px;
	padding-left: 15px;
	margin-right: auto;
	margin-left: auto;
}

.site-content {
	width: 899px;
	min-height: 900px;
	padding: 20px 0 10px 20px;
	float: right;
	border-left: 1px solid #eee;
}
.site-tree, .site-content {
	display: inline-block;
	vertical-align: top;
	font-size: 14px;
}

.gui-main {
	position: relative;
	width: 1140px;
	margin: 0 auto;font-size: 0;/*去除div间距*/
}


.site-tree {
	width: 220px;
	min-height: 900px;
	padding: 5px 0 20px;
}
.site-tree .gui-tree {line-height: 32px;}
.site-tree .gui-tree li { position: relative;}
.site-tree .gui-tree li i {position: relative; font-size: 22px; color: #000}
.site-tree .gui-tree li a cite {padding: 0 8px;}
.site-tree .gui-tree .site-tree-noicon a cite{padding-left: 15px;}
.site-tree .gui-tree li a em{font-size: 12px; color: #bbb; padding-right: 5px; font-style: normal;}
.site-tree .gui-tree li h2{line-height: 36px; border-left: 5px solid #337ab7; margin: 15px 0 5px; padding: 0 10px; background-color: #f2f2f2;font-size:14px;}
.site-tree .gui-tree li ul{margin-left: 27px; line-height: 28px;}
.site-tree .gui-tree li ul a,
.site-tree .gui-tree li ul a i{color: #777;}
.site-tree .gui-tree li ul a:hover{color: #333;}
.site-tree .gui-tree li ul li{margin-left: 25px; overflow: visible; list-style-type: disc; /*list-style-position: inside;*/}
.site-tree .gui-tree li ul li cite,
.site-tree .gui-tree .site-tree-noicon ul li cite{padding-left: 0;}

.site-tree .gui-tree .gui-this a{color: #01AAED;}
.site-tree .gui-tree .gui-this .gui-icon{color: #01AAED;}

.site-fix .site-tree{position: fixed; top: 0; bottom: 0; z-index: 666; min-height: 0; overflow: auto;  background-color: #fff;}
.forScroll {position: fixed;top: 0;}

.site-tree .gui-tree li:before {
	content: "";
	position: absolute;
	z-index: 1;
	right: -2px;
	bottom: 50%;
	top: 50%;
	background: #ff2502;
	width: 3px;
	-webkit-transition-property: top, bottom;
	transition-property: top, bottom;
	-webkit-transition-duration: 0.3s;
	transition-duration: 0.3s;
	-webkit-transition-timing-function: ease-out;
	transition-timing-function: ease-out;
}
.site-tree .gui-tree li:hover:before, .site-tree .gui-tree li:focus:before, .site-tree .gui-tree li:active:before , .site-tree .gui-tree li.active:before{
	top: 0;
	bottom: 0;
}


/*标题*/
.site-title{ margin: 30px 0 20px;}
.site-title fieldset{border: none; padding: 0; border-top: 1px solid #eee;}
.site-title fieldset legend{margin-left: 20px;  padding: 0 10px; font-size: 22px; font-weight: 300;}


.site-doc-color{font-size: 0;}
.site-doc-color li{display: inline-block; vertical-align: middle; width: 180px; margin-left: 20px; margin-bottom: 20px; padding: 20px 10px; color: #fff; text-align: center; border-radius: 2px; line-height: 22px; font-size: 14px;}
.site-doc-color li p[tips]{opacity: 0.8; font-size: 12px;}
.site-doc-necolor li{width: 108px; margin-top: 15px; margin-left: 0; border-radius: 0;}








