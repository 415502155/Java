        btn : btnText,
        anim : 5,
        zIndex : layer.zIndex, // 重点1
        success : function(layero) {
            layer.setTop(layero); // 重点2
        }
    };
    // 传参数
    for (var n = 0; n < arguments[4].length; n++) {
        config["btn" + (n + 1)] = arguments[5][n];
    }
    layer.open(config);
}


