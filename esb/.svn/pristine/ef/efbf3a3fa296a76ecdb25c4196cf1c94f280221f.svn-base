package cn.edugate.esb.comparator;

import java.util.Comparator;
import cn.edugate.esb.entity.Grade;

public class GradeToSortComparator implements Comparator<Grade>{

	@Override  
	public int compare(Grade o1, Grade o2) {  
	  // TODO Auto-generated method stub  
	  int s1=o1.getGrade_id();  
	  int s2=o2.getGrade_id();  
	  if(s1>s2) {  
	    return 1;  
	  } else {  
	    return -1;  
	  }  
	} 
}
