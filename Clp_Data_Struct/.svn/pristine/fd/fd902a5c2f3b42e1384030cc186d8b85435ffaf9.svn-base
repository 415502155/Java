package com.bestinfo.bean.encoding;

import java.io.Serializable;

/**
 * 资金交易类型
 */
public class TradeType implements Serializable {

    private static final long serialVersionUID = -5523427339571706252L;
    private Integer trade_type;  //交易类型编号
    private String trade_type_name; //交易类型名称
    private Integer in_item;//是否收入项
    private Integer batch_item;//是否批量处理
    private Integer work_status; //工作状态

    /**
     * @return the trade_type
     */
    public Integer getTrade_type() {
        return trade_type;
    }

    /**
     * @param trade_type the trade_type to set
     */
    public void setTrade_type(Integer trade_type) {
        this.trade_type = trade_type;
    }

    /**
     * @return the trade_type_name
     */
    public String getTrade_type_name() {
        return trade_type_name;
    }

    /**
     * @param trade_type_name the trade_type_name to set
     */
    public void setTrade_type_name(String trade_type_name) {
        this.trade_type_name = trade_type_name;
    }

    /**
     * @return the work_status
     */
    public Integer getWork_status() {
        return work_status;
    }

    /**
     * @param work_status the work_status to set
     */
    public void setWork_status(Integer work_status) {
        this.work_status = work_status;
    }

    public Integer getIn_item() {
        return in_item;
    }

    public void setIn_item(Integer in_item) {
        this.in_item = in_item;
    }

    public Integer getBatch_item() {
        return batch_item;
    }

    public void setBatch_item(Integer batch_item) {
        this.batch_item = batch_item;
    }

}
