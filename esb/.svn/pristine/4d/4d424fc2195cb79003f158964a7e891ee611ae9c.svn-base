package cn.edugate.esb.comparator;

import java.util.Comparator;

import cn.edugate.esb.entity.Grade;

public class GradeSorter implements Comparator<Grade>{
	@Override  
	public int compare(Grade c1, Grade c2) {  
	  int s1=c1.getGrade_number(); 
	  int s2=c2.getGrade_number();   
	  if(s1>s2) {  
	    return 1;  
	  } else {  
	    return -1;  
	  }
	} 
}
