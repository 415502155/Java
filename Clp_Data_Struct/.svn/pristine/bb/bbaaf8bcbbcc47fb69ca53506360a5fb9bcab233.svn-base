package com.bestinfo.bean.game;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 奖级表
 */
public class GameClassInfo implements Serializable {

    private static final long serialVersionUID = -3079102780418801946L;
    private Integer game_id;//游戏编号
    private Integer play_id;//玩法编号
    private Integer class_id;//奖级编号
    private String class_name;//奖级名称
    private Integer fix_mark;//固定奖级
    private BigDecimal prize_proportion;//奖金和比例
    private Integer last_relation;//与下级关系
    private BigDecimal last_diff;//差额或倍数
    private BigDecimal top_money;//封顶金额
    private Integer open_id;//开奖次数
    private Integer work_status;//奖级状态

    //显示游戏名称的中文名称
    private String game_name;
    //显示玩法的中文名称
    private String play_name;

    //added by yangyuefu
    private Integer prizeNumber;//中奖注数

    public Integer getGame_id() {
        return game_id;
    }

    public void setGame_id(Integer game_id) {
        this.game_id = game_id;
    }

    public Integer getPlay_id() {
        return play_id;
    }

    public void setPlay_id(Integer play_id) {
        this.play_id = play_id;
    }

    public Integer getClass_id() {
        return class_id;
    }

    public void setClass_id(Integer class_id) {
        this.class_id = class_id;
    }

    public String getClass_name() {
        return class_name;
    }

    public void setClass_name(String class_name) {
        this.class_name = class_name;
    }

    public Integer getFix_mark() {
        return fix_mark;
    }

    public void setFix_mark(Integer fix_mark) {
        this.fix_mark = fix_mark;
    }

    public BigDecimal getPrize_proportion() {
        return prize_proportion;
    }

    public void setPrize_proportion(BigDecimal prize_proportion) {
        this.prize_proportion = prize_proportion;
    }

    public Integer getLast_relation() {
        return last_relation;
    }

    public void setLast_relation(Integer last_relation) {
        this.last_relation = last_relation;
    }

    public BigDecimal getLast_diff() {
        return last_diff;
    }

    public void setLast_diff(BigDecimal last_diff) {
        this.last_diff = last_diff;
    }

    public BigDecimal getTop_money() {
        return top_money;
    }

    public void setTop_money(BigDecimal top_money) {
        this.top_money = top_money;
    }

    public Integer getOpen_id() {
        return open_id;
    }

    public void setOpen_id(Integer open_id) {
        this.open_id = open_id;
    }

    public Integer getWork_status() {
        return work_status;
    }

    public void setWork_status(Integer work_status) {
        this.work_status = work_status;
    }

    /**
     * @return the game_name
     */
    public String getGame_name() {
        return game_name;
    }

    /**
     * @param game_name the game_name to set
     */
    public void setGame_name(String game_name) {
        this.game_name = game_name;
    }

    /**
     * @return the play_name
     */
    public String getPlay_name() {
        return play_name;
    }

    /**
     * @param play_name the play_name to set
     */
    public void setPlay_name(String play_name) {
        this.play_name = play_name;
    }

    public Integer getPrizeNumber() {
        return prizeNumber;
    }

    public void setPrizeNumber(Integer prizeNumber) {
        this.prizeNumber = prizeNumber;
    }

}
