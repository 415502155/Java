button,input,textarea {
	outline: 0;
	-webkit-transition: border-color .3s cubic-bezier(.65,.05,.35,.5);
	transition: border-color .3s cubic-bezier(.65,.05,.35,.5);
	-webkit-box-sizing: border-box!important;
	-moz-box-sizing: border-box!important;
	box-sizing: border-box!important
}

/*徽章 gui-badge*/

.gui-badge {
	display: inline-block;
	min-width: 10px;
	padding: 3px 5px;
	color: #fff;
	line-height: 1;
	vertical-align: baseline;
	white-space: nowrap;
	text-align: center;
	background-color: #999;
	border-radius: 10px;
	margin-left: 3px;
}

.gui-badge:empty {
	display: none
}
.gui-badge.gui-square {
	border-radius: 0
}

.gui-badge.gui-radius {
	border-radius: 2px
}

.gui-badge.gui-round {
	border-radius: 1000px
}

a.gui-badge:focus,a.gui-badge:hover {
	color: #fff;
	text-decoration: none;
	cursor: pointer
}

.gui-badge-primary {
	background-color: #0e90d2
}

.gui-badge-secondary {
	background-color: #3bb4f2
}

.gui-badge-success {
	background-color: #5eb95e
}

.gui-badge-warning {
	background-color: #F37B1D
}

.gui-badge-danger {
	background-color: #F30
}
.gui-badge-tip {
	background-color: #F30;
	position: absolute;
	/* -webkit-transform: translateX(-50%); */
	/*-ms-transform: translateX(-50%);*/
	/* transform: translateX(-50%); */
	top: -10px;
	height: 20px;
	border-radius: 10px;
	/* min-width: 20px; */
	background: #f04134;
	color: #fff;
	line-height: 20px;
	text-align: center;
	padding: 0 6px;
	font-size: 12px;
	white-space: nowrap;
	-webkit-transform-origin: -10% center;
	-ms-transform-origin: -10% center;
	transform-origin: -10% center;
	font-family: tahoma;
	box-shadow: 0 0 0 1px #fff;
}


/*表格*/
/*************
  Blue Theme
 *************/
/* overall */
.gui-table {
	width: 100%;

	/*  margin: 10px 0 15px; */
	text-align: left;
	border-spacing: 0;
	border: #e0e0e0 1px solid;
	border-width: 1px 0 0 1px;
}

.gui-table th, .gui-table td {
	border: #e0e0e0  1px solid;
	border-width: 0 1px 1px 0;
}

/* header */
.gui-table th, .gui-table thead td {
	color: #5a728a;
	background-color: #f7f7f7;
	border-collapse: collapse;
	padding: 4px;
	/* font-size: 16px; */
	/* height: 42px; */
	/* line-height: 42px; */
	text-align: center;
	font-weight: normal;
}

.gui-table tbody td, .gui-table tfoot th, .gui-table tfoot td {
	padding: 4px;
	vertical-align: top;
}
/* .gui-table tbody tr {cursor:pointer;}*/
.gui-table .header, .gui-table .tablesorter-header {
	/* black (unsorted) double arrow */
	background-image: url(data:image/gif;base64,R0lGODlhFQAJAIAAACMtMP///yH5BAEAAAEALAAAAAAVAAkAAAIXjI+AywnaYnhUMoqt3gZXPmVg94yJVQAAOw==);
	/* white (unsorted) double arrow */
	/* background-image: url(data:image/gif;base64,R0lGODlhFQAJAIAAAP///////yH5BAEAAAEALAAAAAAVAAkAAAIXjI+AywnaYnhUMoqt3gZXPmVg94yJVQAAOw==); */
	/* image */
	/* background-image: url(images/black-unsorted.gif); */
	background-repeat: no-repeat;
	background-position: center right;
	padding: 4px 18px 4px 4px;
	white-space: normal;
	cursor: pointer;
}

.gui-table .headerSortUp, .gui-table .tablesorter-headerSortUp, .gui-table .tablesorter-headerAsc {
	background-color: #9fbfdf;
	/* black asc arrow */
	background-image: url(data:image/gif;base64,R0lGODlhFQAEAIAAACMtMP///yH5BAEAAAEALAAAAAAVAAQAAAINjI8Bya2wnINUMopZAQA7);
	/* white asc arrow */
	/* background-image: url(data:image/gif;base64,R0lGODlhFQAEAIAAAP///////yH5BAEAAAEALAAAAAAVAAQAAAINjI8Bya2wnINUMopZAQA7); */
	/* image */
	/* background-image: url(images/black-asc.gif); */
}

.gui-table .headerSortDown, .gui-table .tablesorter-headerSortDown, .gui-table .tablesorter-headerDesc {
	background-color: #8cb3d9;
	/* black desc arrow */
	background-image: url(data:image/gif;base64,R0lGODlhFQAEAIAAACMtMP///yH5BAEAAAEALAAAAAAVAAQAAAINjB+gC+jP2ptn0WskLQA7);
	/* white desc arrow */
	/* background-image: url(data:image/gif;base64,R0lGODlhFQAEAIAAAP///////yH5BAEAAAEALAAAAAAVAAQAAAINjB+gC+jP2ptn0WskLQA7); */
	/* image */
	/* background-image: url(images/black-desc.gif); */
}

.gui-table thead .sorter-false {
	background-image: none;
	cursor: default;
	padding: 4px;
}

/* tfoot */
.gui-table tfoot .tablesorter-headerSortUp, .gui-table tfoot .tablesorter-headerSortDown, .gui-table tfoot .tablesorter-headerAsc, .gui-table tfoot .tablesorter-headerDesc {
	/* remove sort arrows from footer */
	background-image: none;
}

/* tbody */
.gui-table td {
	color: #3d3d3d;
	padding: 4px;
	vertical-align: top;
	height: 30px;
	line-height: 30px;
	text-align: center;
	color: #888;
	font-size: 14px;
}
.gui-table td.text-left {
	text-align: left;
}
.gui-table td.text-right {
	text-align: right;
}

/* hovered row colors
 you'll need to add additional lines for
 rows with more than 2 child rows
 鼠标滑过效果
 */
.gui-table tbody > tr.hover > td, .gui-table tbody > tr:hover > td, .gui-table tbody > tr:hover + tr.tablesorter-childRow > td, .gui-table tbody > tr:hover + tr.tablesorter-childRow + tr.tablesorter-childRow > td, .gui-table tbody > tr.even.hover > td, .gui-table tbody > tr.even:hover > td, .gui-table tbody > tr.even:hover + tr.tablesorter-childRow > td, .gui-table tbody > tr.even:hover + tr.tablesorter-childRow + tr.tablesorter-childRow > td {

	background-color: #e7f0f5;
	color: #2f5896;
}

.gui-table tbody > tr.odd.hover > td, .gui-table tbody > tr.odd:hover > td, .gui-table tbody > tr.odd:hover + tr.tablesorter-childRow > td, .gui-table tbody > tr.odd:hover + tr.tablesorter-childRow + tr.tablesorter-childRow > td {
	background-color: #e7f0f5;
	color: #2f5896;
}

/* table processing indicator */
.gui-table .tablesorter-processing {
	background-position: center center !important;
	background-repeat: no-repeat !important;
	/* background-image: url(images/loading.gif) !important; */
	background-image: url('data:image/gif;base64,') !important;
}

/* Zebra Widget - row alternating colors */
.gui-table tbody tr.odd > td {
	background-color: #fafbff;
}

.gui-table tbody tr > td.enRated {color: #e34a4a;}
.gui-table tbody tr > td.operation {color: #337ab7;width: 240px; }
.gui-table tbody tr > td.operation span { padding:0 5px 0 10px; cursor: pointer; border-right: 1px #e0e0e0 solid; position: relative;}
.gui-table tbody tr > td.operation span:last-child {border-right: none;}

/*鼠标滑过动画*/
.gui-table tbody tr > td.operation span:hover {color: #f2564e; }
.gui-table tbody tr > td.operation span em{ display:block; position:absolute; width:0px; height:0px; background:#f2564e; top:24px; left:50%; transition: all .3s ease-out;}
.gui-table tbody tr > td.operation span:hover em { width:100%; height:1px; background:#f2564e;left:0;right:0;}


.gui-table tbody tr.even > td {
	background-color: #fff;
}

/* Column Widget - column sort colors */
.gui-table td.primary, .gui-table tr.odd td.primary {
	background-color: #99b3e6;
}

.gui-table tr.even td.primary {
	background-color: #c2d1f0;
}
/*班级*/
.gui-table tbody tr td.className {
	background: rgb(232, 238, 244);

}
.gui-table td.secondary, .gui-table tr.odd td.secondary {
	background-color: #c2d1f0;
}

.gui-table tr.even td.secondary {
	background-color: #d6e0f5;
}

.gui-table td.tertiary, .gui-table tr.odd td.tertiary {
	background-color: #d6e0f5;
}

.gui-table tr.even td.tertiary {
	background-color: #ebf0fa;
}

/* caption */
.caption {
	background-color: #fff;
}

/* filter widget */
.gui-table .tablesorter-filter-row {
	background-color: #eee;
}

.gui-table .tablesorter-filter-row td {
	background-color: #eee;
	line-height: normal;
	text-align: center;
	/* center the input */
	-webkit-transition: line-height 0.1s ease;
	-moz-transition: line-height 0.1s ease;
	-o-transition: line-height 0.1s ease;
	transition: line-height 0.1s ease;
}

/* optional disabled input styling */
.gui-table .tablesorter-filter-row .disabled {
	opacity: 0.5;
	filter: alpha(opacity=50);
	cursor: not-allowed;
}

/* hidden filter row */
.gui-table .tablesorter-filter-row.hideme td {
	/*** *********************************************** ***/
	/*** change this padding to modify the thickness     ***/
	/*** of the closed filter row (height = padding x 2) ***/
	padding: 2px;
	/*** *********************************************** ***/
	margin: 0;
	line-height: 0;
	cursor: pointer;
}

.gui-table .tablesorter-filter-row.hideme * {
	height: 1px;
	min-height: 0;
	border: 0;
	padding: 0;
	margin: 0;
	/* don't use visibility: hidden because it disables tabbing */
	opacity: 0;
	filter: alpha(opacity=0);
}

/* filters */
.gui-table input.tablesorter-filter, .gui-table select.tablesorter-filter {
	width: 98%;
	height: auto;
	margin: 0;
	padding: 4px;
	background-color: #fff;
	border: 1px solid #bbb;
	color: #333;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-transition: height 0.1s ease;
	-moz-transition: height 0.1s ease;
	-o-transition: height 0.1s ease;
	transition: height 0.1s ease;
}

/* rows hidden by filtering (needed for child rows) */
.tablesorter .filtered {
	display: none;
}

/* ajax error row */
.tablesorter .tablesorter-errorRow td {
	text-align: center;
	cursor: pointer;
	background-color: #e6bf99;
}




/*
 * 分页
 *
 * 面包屑
 *
 * */
.gui-breadcrumb {
	padding: .7em .5em;
	margin-bottom: 2rem;
	list-style: none;
	background-color: transparent;
	border-radius: 0;
	font-size: 14px
}

.gui-breadcrumb>li {
	display: inline-block
}

.gui-breadcrumb>li [class*=gui-icon-]:before {
	color: #999;
	margin-right: 5px
}

.gui-breadcrumb>li+li:before {
	content: "/";
	padding: 0 8px;
	color: #ccc;
}

.gui-breadcrumb>.gui-active {
	color: #999
}

.gui-breadcrumb-slash>li+li:before {
	content: "/\00a0";
}


/*
 * 按钮
 * 按钮 gui-btn样式
 * 6个类型按钮
 *
 */
.gui-btn {
	display: inline-block;
	height: 36px;
	line-height: 36px;
	padding: 0 16px;
	white-space: nowrap;
	text-align: center;
	border: none;
	border-radius: 2px;
	cursor: pointer;
	opacity: .9;
	filter: alpha(opacity=90);
	font-size: 14px;
}
.gui-btn[disabled],.gui-btn[disabled]:hover,.gui-btn[disabled]:active  {
	opacity: .6;
	cursor: not-allowed;
}


.gui-btn:active {
	opacity: 1;
	filter: alpha(opacity=100)
}

/*.gui-btn+.gui-btn {
    margin-left: 10px
}*/

.gui-btn-radius {
	border-radius: 100px
}

.gui-btn .gui-icon {
	font-size: 18px;
	vertical-align: bottom
}

/*线框按钮*/
.gui-btn-primary {
	border: 1px solid #C9C9C9;
	background-color: #fff;
	color: #555;
	line-height: 34px;
}
.gui-btn-primary:hover{
	background-color: #fff;
	color:#337ab7;
	border: 1px solid #337ab7;
}
/*灰色按钮*/
.gui-btn-gray,.gui-btn-gray-disabled {
	background-color: #e6e6e6;
	border-color: #e6e6e6;
	color: #444;
}
.gui-btn-gray:hover {
	background-color: #c7c7c7;
	color: #444;
}
/*默认蓝色按钮*/
.gui-btn-normal,.gui-btn-normal-disabled {
	background-color: #337ab7;color: #fff;
}
.gui-btn-normal:hover {
	background: #00a2d4;
	color: #fff
}
/*绿色按钮*/
.gui-btn-success,.gui-btn-success-disabled {
	background-color: #37c388;color: #fff;
}
.gui-btn-success:hover {
	background-color: #21a56d
}
/*黄色按钮*/
.gui-btn-warm,.gui-btn-warm-disabled {
	background-color: #F7B824;color: #fff;
}
.gui-btn-warm:hover {
	background-color: #F37B1D
}
/*红色按钮*/
.gui-btn-danger, .gui-btn-danger-disabled{
	background-color: #FF5722;color: #fff;
}
.gui-btn-danger:hover {
	background-color: #FF3300
}

.gui-btn-link, .gui-btn-link-disabled{
	color: #337ab7;
	background: none;
}
.gui-btn-link:hover {
	color: #009ad9; text-decoration: underline;
}
/*禁用样式*/
.gui-btn-disabled,.gui-btn-disabled:active,.gui-btn-disabled:hover {
	border: 1px solid #e6e6e6;
	background-color: #FBFBFB;
	color: #C9C9C9;
	cursor: not-allowed;
	opacity: 1
}

/*大尺寸按钮*/
.gui-btn-big {
	height: 44px;
	line-height: 44px;
	padding: 0 25px;
	font-size: 16px
}
/*小尺寸按钮*/
.gui-btn-small {
	height: 30px;
	line-height: 30px;
	padding: 0 10px;
	font-size: 12px
}

.gui-btn-small i {
	font-size: 16px!important
}

.gui-btn-mini {
	height: 22px;
	line-height: 22px;
	padding: 0 5px;
	font-size: 12px
}

.gui-btn-mini i {
	font-size: 14px!important
}
/*按钮组*/
.gui-btn-group {
	display: inline-block;
	vertical-align: middle;
	font-size: 0;
	position: relative;
}
.gui-btn-group .gui-btn {
	margin-left: 0!important;
	margin-right: 0!important;
	border-left: 1px solid rgba(255,255,255,.5);
	border-radius: 0;
	font-size: 14px;
}
.gui-btn-group .gui-btn-primary {
	border-left: none
}
.gui-btn-group .gui-btn-primary:hover {
	border-color: #00a2d4;
	background: #00a2d4;
	color: #fff; margin-left: -1px;
}
.gui-btn-group .gui-btn:first-child {
	border-left: none;
	border-radius: 2px 0 0 2px
}
.gui-btn-group .gui-btn-primary:first-child {
	border-left: 1px solid #c9c9c9
}

.gui-btn-group .gui-btn:last-child {
	border-radius: 0 2px 2px 0
}
.gui-btn-group .gui-btn-active { line-height: 28px;}
.gui-btn-group .gui-btn-primary-active {
	color: #fff;
	background: #9bbedc;
	border: 1px #8ab3d6 solid
}

.gui-btn-group .gui-btn+.gui-btn {
	margin-left: 10px;
}

.gui-btn-group+.gui-btn-group {
	margin-left: 10px
}

/*按钮下拉菜单*/
.gui-btn-group .dropdown-menu {
	position: absolute;
	top: 100%;
	left: 0;
	z-index: 1000;
	display: none;
	float: left;
	min-width: 160px;
	padding: 5px 0;
	margin: 2px 0 0;
	font-size: 14px;
	text-align: left;
	list-style: none;
	background-color: #fff;
	-webkit-background-clip: padding-box;
	background-clip: padding-box;
	border: 1px solid #ccc;
	border: 1px solid rgba(0,0,0,.15);
	border-radius: 4px;
	-webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175);
	box-shadow: 0 6px 12px rgba(0,0,0,.175);
}
.gui-btn-group .dropdown-menu .divider {
	height: 1px;
	margin: 9px 0;
	overflow: hidden;
