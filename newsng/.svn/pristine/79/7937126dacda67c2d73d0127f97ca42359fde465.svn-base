@charset "utf-8";
/* CSS Document */
/*数据统计*/
.dataStatistics {
	margin-top: -44px;
	text-align: right;
	padding-right: 10px;
}
 .numberShow { color: #cc5200;min-width: 24px;display: inline-block; text-align: center}
/*数据操作*/
.dataStatistics .numberShow i {color: #c0c0c0}
.dataTool {clear: both; height: 100px;border-top: 1px #ccc dotted;padding-top: 18px;margin-top: 24px; color: #b3b3b3}
.dataTool input {margin-top: -1px;vertical-align: middle;margin-right: 8px;}
.dataTool label { line-height: 32px;}
/*
* form
* 表单样式
* 包含报错信息
*
* */
.gui-form .gui-form-item-label {
	text-align: right;
	vertical-align: middle;
	float: left;
	font-size: 14px;
	color: #555555;
	line-height: 1;
	padding: 10px 12px 10px 0;
	box-sizing: border-box;
	width: 80px;
}
.gui-select {
	display: inline-block;
	height: 32px;
	line-height: 1.5;
	padding: 4px 7px;
	font-size: 14px;
	border: 1px solid #e2e2e2;
	border-radius: 3px;
	color: #657180;
	background-color: #fff;
	background-image: none;
	position: relative;
	cursor: text;
	transition: border .2s ease-in-out,background .2s ease-in-out,box-shadow .2s ease-in-out;
	max-width: 300px;
}
.gui-form-item-content {
	position: relative;
	font-size: 14px;
	margin-left: 90px;
	color: #9e9e9e;
	line-height: 34px;
}

.gui-form-item-content button {
    line-height: 18px;
}

.readonlyInput {
	border: none;
	color: #969696;
}

.line {
	border-bottom: 1px #ccc dotted;
	margin: 20px 0;
}


/*开关*/
[class|=switch] {
	position: relative;
	display: inline-block;
	width: 40px;
	height: 24px;
	border-radius: 16px;
	line-height: 32px;
	-webkit-tap-highlight-color: transparent
}

.switch-on {
	border: 1px solid #fff;
	box-shadow: #fff 0 0 0 16px inset;
	transition: border .4s,box-shadow .2s,background-color 1.2s;
	background-color: #fff;
	cursor: pointer
}

.slider {
	position: absolute;
	display: inline-block;
	width: 22px;
	height: 22px;
	background: #fff;
	box-shadow: 0 1px 3px rgba(0,0,0,.4);
	border-radius: 50%;
	left: 0;
	top: 0
}

.switch-on .slider {
	left: 15px;
	transition: background-color .4s,left .2s
}

.switch-off {
	border: 1px solid #dfdfdf;
	transition: border .4s,box-shadow .4s;
	background-color: #fff;
	box-shadow: #dfdfdf 0 0 0 0 inset;
	background-color: #fff;
	cursor: pointer
}

.switch-off .slider {
	left: 0;
	transition: background-color .4s,left .2s
}

.switch-on.switch-disabled {
	opacity: .5;
	cursor: auto
}

.switch-off.switch-disabled {
	background-color: #f0f0f0!important;
	cursor: auto
}

/*表单里单选 及复选框*/
.form-label { line-height: 34px; margin-right: 24px;}
.form-label input { vertical-align: middle}

.formlink {
	color:#2d8cf0;
	cursor: pointer;
	display: inline-block;
}

/*表单报错*/
.gui-form-input-error {
	outline: 0;
	box-shadow: 0 0 0 2px rgba(255, 51, 0, 0.15);
	border: 1px #f30 solid;
	background-color:#fff5f3 ;

}
.gui-form-input-error:focus {
	border: 1px solid #f30;
	outline: 0;
	box-shadow: 0 0 0 2px rgba(255, 51, 0, 0.15);
	border: 1px #f30 solid;
	background-color:#fff5f3 ;
}

.gui-form-item-error {
	color: #f30;
	margin-left: 15px;
	font-size: 14px;
}

input[data-date='YYYY-MM-DD'] {
	background-image:url(../images/datebg.png);
	background-position:96px 6px ;
	background-repeat:no-repeat;
	width: 120px;
}
input[data-date='YYYY-MM-DD hh:mm:ss']{
	background-image: url(../images/datebg.png);
	background-position:135px 6px;
	background-repeat:no-repeat;
	width: 160px;
}


/**选择教师窗口*/
.tech_Wrap {line-height: 32px; padding: 0 20px;}
.tech_Wrap ul li { cursor: pointer; position: relative}
.tech_Wrap ul li.changed {color: #ccc}
.tech_Wrap ul li.changed i.fa-user {color: #ccc}
.tech_Wrap .tech_item i.fa-user {
	color: #b6daf1;
	font-size: 13px;
	margin: 3px;
}
.tech_Wrap .tech_item { height: 300px; width: 330px; position: relative; float: left; padding: 20px; }
.tech_Wrap .tech_item .fa-times-circle, .tech_Wrap .tech_item  .fa-plus-square-o {
	position: absolute;
	top: 10px;
	right: 20px;
	font-size: 14px;
	color: #d2d2d2;
}

.tech_Wrap .tech_item  .fa-plus-square-o { display: none}

.tech_Wrap .tech_item ul {
	height: 320px;overflow: auto;
}
/*滚动条样式*/
.tech_Wrap .tech_item ul::-webkit-scrollbar {
	width: 4px;
}
.tech_Wrap .tech_item ul::-webkit-scrollbar-thumb {
	background-color:#d9d9d9;
	border-radius: 2px;
}

.tech_Wrap .tech_item ul li:hover { color: #a1c5ea;}
.tech_Wrap .tech_item ul li:hover .fa-times-circle { color: #ff6600;display: block}
.tech_Wrap .tech_item ul li.unChanged:hover .fa-plus-square-o {display:block;color:#a1c5ea}
.tech_Wrap .tech_item ul li.changed:hover {color: #cccccc}
.tech_Wrap .dialogSearch {padding-bottom: 10px; position: relative}
.tech_Wrap .dialogSearch input {width: 240px;}
.tech_Wrap .dialogSearch div {
	width: 32px;
	height: 32px;
	position: absolute;
	top: 0px;
	right:53px;
	text-align: center;
	cursor: pointer;
}
.tech_Wrap  #searchName { width: 290px}
.tech_Wrap  #unChangedT { border-right: 1px #e8e8e8 solid }
.tech_Wrap .changedTtitle { height:42px;font-size: 14px;font-weight: bold;}

.gui-alert .fa-times {
	position: absolute;
	right: 0px;
	padding: 10px;
	top: 0;
	cursor: pointer;
}

/*统计选择结果*/
.changeSum {margin-right:10px;}
.changeSum span {margin:0px 3px;}
.backIcon {
	background: #f2f2f2;
	border-radius: 4px;
	width: 40px;
	height: 40px;
	box-shadow: 0 1px 3px rgba(26, 26, 26, 0.1);
	position: fixed;
	bottom: 100px;
	right: 18px;
	display: flex;
	flex-direction: column;
	transition: -webkit-transform 0.2s ease;
	transition: all 0.2s ease;
	transition: all 0.2s ease, -webkit-transform 0.2s ease;
	will-change: transfrom;
	z-index: 200;
	text-align: center;
	padding: 6px 10px;
	font-size: 24px;
	border: #e6e6e6 1px solid;
	cursor: pointer;
	color: #337ab7;
	opacity: .48;
}

.backIcon:hover {
	opacity: .9;
}
